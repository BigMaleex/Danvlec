<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane fx:id="APMain" maxHeight="400.0" minHeight="120.0" minWidth="100.0" prefWidth="620.0" stylesheets="@../CSS/PopupOneButton.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PopupTwoButtonController">

    <children>
        <VBox fx:id="rootVBox" alignment="TOP_CENTER" prefWidth="560.0" spacing="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <padding>
                <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
            </padding>
            <children>
                <ImageView fx:id="IMGIcon" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" />
                <ScrollPane fx:id="SPContent" fitToWidth="true" hbarPolicy="NEVER" maxHeight="225.0" minHeight="10.0" prefWidth="560.0">
                    <content>
                        <VBox>
                            <children>
                                <TextFlow fx:id="TFLHeader" />
                                <TextFlow fx:id="TFLContent" />
                                <TextFlow fx:id="TFLFooter" />
                            </children>
                        </VBox>
                    </content>
                </ScrollPane>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Button fx:id="BTNCancel" layoutX="261.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#BTNCancelOnMouseClicked" onMouseEntered="#BTNCancelOnMouseEntered" onMouseExited="#BTNCancelOnMouseExited" text="X Cerrar">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="BTNAccept" mnemonicParsing="false" onMouseClicked="#BTNAcceptOnMouseClicked" onMouseEntered="#BTNAcceptOnMouseEntered" onMouseExited="#BTNAcceptOnMouseExited" text="Aceptar">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
